<Styles>
	<Style name="Image on Left" element="img">
		<Attribute name="style" value="padding: 5px; margin-right: 5px" />
		<Attribute name="border" value="2" />
		<Attribute name="align" value="left" />
	</Style>
	<Style name="Image on Right" element="img">
		<Attribute name="style" value="padding: 5px; margin-left: 5px" />
		<Attribute name="border" value="2" />
		<Attribute name="align" value="right" />
	</Style>
	<Style name="Header 1" element="h1">
		<Attribute name="class" value="dhis-cde_form_h1" />
	</Style>
	<Style name="Header 2" element="h2">
		<Attribute name="class" value="dhis-cde_form_h2" />
	</Style>
	<Style name="Header 3" element="h3">
		<Attribute name="class" value="dhis-cde_form_h3" />
	</Style>
	<Style name="Header 4" element="h4">
		<Attribute name="class" value="dhis-cde_form_h4" />
	</Style>
	<Style name="Standard label" element="span">
		<Attribute name="class" value="dhis-cde_form_stlabel" />
	</Style>
	<Style name="Small label" element="span">
		<Attribute name="class" value="dhis-cde_form_smlabel" />
	</Style>
	<Style name="Annontations" element="span">
		<Attribute name="class" value="dhis-cde_form_anno" />
	</Style>
</Styles>