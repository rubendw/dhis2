<dl>
	<dt>&#3370;&#3399;&#3376;&#3405;</dt><dd>&#3384;&#3405;&#3381;&#3375;&#3330; &#3381;&#3391;&#3381;&#3376;&#3363;&#3390;&#3364;&#3405;&#3374;&#3349;&#3374;&#3390;&#3363;&#3405;</dd>
	<dt>&#3381;&#3391;&#3381;&#3376;&#3363;&#3330;</dt><dd>&#3374;&#3377;&#3405;&#3377;&#3393; &#3349;&#3394;&#3359;&#3393;&#3364;&#3378;&#3405;&#8205; &#3381;&#3391;&#3381;&#3376;&#3353;&#3405;&#3353;&#3379;&#3405;&#8205;,&#3333;&#3364;&#3391;&#3368;&#3405;&#3377;&#3398; &#3337;&#3370;&#3375;&#3403;&#3351;&#3330; &#3342;&#3368;&#3405;&#3368;&#3391;&#3381; &#3370;&#3405;&#3376;&#3364;&#3391;&#3370;&#3390;&#3366;&#3391;&#3349;&#3405;&#3349;&#3393;&#3368;&#3405;&#3368;&#3393;..</dd>
	<dt>&#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3364;&#3376;&#3330;</dt><dd>&#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3349;&#3363;&#3349;&#3405;&#3349;&#3390;&#3349;&#3405;&#3349;&#3390;&#3368;&#3405;&#8205; &#3337;&#3370;&#3375;&#3403;&#3351;&#3391;&#3349;&#3405;&#3349;&#3393;&#3368;&#3405;&#3368;&#3393;.&#3337;&#3366;&#3390;: % &#3346;&#3376;&#3393; &#3364;&#3376;&#3374;&#3390;&#3363;&#3405;.</dd>
</dl>
<p>&#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3376;&#3394;&#3370;&#3392;&#3349;&#3376;&#3391;&#3349;&#3405;&#3349;&#3393;&#3368;&#3405;&#3368;&#3364;&#3391;&#3368;&#3393; &#3374;&#3393;&#3368;&#3405;&#8205;&#3370;&#3405; &#3364;&#3368;&#3405;&#3368;&#3399; &#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3364;&#3376;&#3330; &#3337;&#3363;&#3405;&#3359;&#3390;&#3349;&#3405;&#3349;&#3363;&#3330;.&#3349;&#3394;&#3359;&#3405;&#3359;&#3393;&#3349;/&#3364;&#3391;&#3376;&#3393;&#3364;&#3405;&#3364;&#3393;&#3349;/&#3374;&#3390;&#3377;&#3405;&#3377;&#3393;&#3349;/&#3349;&#3390;&#3363;&#3393;&#3349; &#3342;&#3368;&#3405;&#3368;&#3391;&#3381; &#3354;&#3398;&#3375;&#3405;&#3375;&#3393;&#3368;&#3405;&#3368;&#3364;&#3391;&#3378;&#3399;&#3349;&#3405;&#3349;&#3390;&#3375;&#3391; &#3335;&#3359;&#3364;&#3393; &#3373;&#3390;&#3351;&#3364;&#3405;&#3364;&#3398; &#3374;&#3398;&#3368;&#3393;&#3381;&#3391;&#3378;&#3405;&#8205; &#3368;&#3391;&#3368;&#3405;&#3368;&#3393;&#3330; '&#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3364;&#3376;&#3330;' &#3364;&#3391;&#3376;&#3358;&#3405;&#3358;&#3359;&#3393;&#3349;&#3405;&#3349;&#3393;&#3349;.&#3336; &#3364;&#3376;&#3364;&#3405;&#3364;&#3391;&#3378;&#3405;&#8205; &#3346;&#3376;&#3393; &#3370;&#3399;&#3376;&#3393;&#3330;(&#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3354;&#3399;&#3376;&#3405;&#8205;&#3349;&#3405;&#3349;&#3393;&#3368;&#3405;&#3368;&#3364;&#3391;&#3368;&#3405;),&#3346;&#3376;&#3393; &#3352;&#3359;&#3349;&#3381;&#3393;&#3330;(&#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3349;&#3363;&#3405;&#3349;&#3405;&#3349;&#3390;&#3349;&#3405;&#3349;&#3393;&#3368;&#3405;&#3368;&#3364;&#3391;&#3368;&#3405;)&#3349;&#3390;&#3363;&#3393;&#3330;.&#3337;&#3366;&#3390;&#3385;&#3376;&#3363;&#3374;&#3390;&#3375;&#3391; &#3370;&#3399;&#3376;&#3405; % &#3337;&#3330;  &#3352;&#3359;&#3349;&#3330; &#3431;&#3430;&#3430; &#3337;&#3330;.&#3336; &#3352;&#3359;&#3349;&#3330; &#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205;  &#3371;&#3403;&#3376;&#3405;&#8205;&#3374;&#3393;&#3378;&#3375;&#3403;&#3359;&#3405; &#3351;&#3393;&#3363;&#3391;&#3349;&#3405;&#3349;&#3393;&#3330;&#3370;&#3403;&#3379;&#3405;&#8205; &#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3381;&#3391;&#3378; &#3349;&#3391;&#3359;&#3405;&#3359;&#3393;&#3330;.</p>

<h4>&#3342;&#3353;&#3405;&#3353;&#3368;&#3398; &#3334;&#3363;&#3405; &#3370;&#3393;&#3364;&#3391;&#3375; &#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3349;&#3394;&#3359;&#3405;&#3359;&#3391;&#3354;&#3405;&#3354;&#3399;&#3376;&#3405;&#8205;&#3349;&#3405;&#3349;&#3399;&#3363;&#3405;&#3359;&#3364;&#3405;.</h4>
<p>&#3333;&#3359;&#3391;&#3384;&#3405;&#3365;&#3390;&#3368; &#3370;&#3405;&#3376;&#3381;&#3376;&#3405;&#8205;&#3364;&#3405;&#3364;&#3368;&#3353;&#3405;&#3353;&#3379;&#3390;&#3375; &#3349;&#3394;&#3359;&#3405;&#3359;&#3393;&#3349;/&#3364;&#3391;&#3376;&#3393;&#3364;&#3405;&#3364;&#3393;&#3349;/&#3374;&#3390;&#3377;&#3405;&#3377;&#3393;&#3349;/&#3349;&#3390;&#3363;&#3393;&#3349; &#3342;&#3368;&#3405;&#3368;&#3391;&#3381;&#3349;&#3405;&#3349;&#3393; &#3370;&#3393;&#3377;&#3374;&#3399; &#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3377;&#3391;&#3368;&#3405; &#3336; &#3378;&#3391;&#3384;&#3405;&#3377;&#3405;&#3377;&#3391;&#3378;&#3405;&#8205; &#3376;&#3363;&#3405;&#3359;&#3405; &#3370;&#3405;&#3376;&#3381;&#3376;&#3405;&#8205;&#3364;&#3405;&#3364;&#3368;&#3353;&#3405;&#3353;&#3379;&#3405;&#8205; &#3349;&#3394;&#3359;&#3391; &#3337;&#3363;&#3405;&#3359;&#3405;.&#3346;&#3368;&#3405;&#3368;&#3405; &#3368;&#3405;&#3375;&#3394;&#3374;&#3377;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3371;&#3403;&#3376;&#3405;&#8205;&#3374;&#3393;&#3378; &#3368;&#3391;&#3376;&#3405;&#8205;&#3381;&#3354;&#3391;&#3349;&#3405;&#3349;&#3393;&#3368;&#3405;&#3368;&#3364;&#3391;&#3368;&#3393;&#3330; &#3374;&#3377;&#3405;&#3377;&#3402;&#3368;&#3405;&#3368;&#3405; &#3361;&#3391;&#3368;&#3403;&#3374;&#3391;&#3368;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3371;&#3403;&#3376;&#3405;&#8205;&#3374;&#3393;&#3378; &#3368;&#3391;&#3376;&#3405;&#8205;&#3381;&#3354;&#3391;&#3349;&#3405;&#3349;&#3393;&#3368;&#3405;&#3368;&#3364;&#3391;&#3368;&#3393;&#3330;.&#3346;&#3376;&#3393; &#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3337;&#3363;&#3405;&#3359;&#3390;&#3349;&#3405;&#3349;&#3393;&#3368;&#3405;&#3368;&#3364;&#3391;&#3368;&#3405; "&#3349;&#3394;&#3359;&#3405;&#3359;&#3394;&#3349; &#3342;&#3368;&#3405;&#3368; &#3376;&#3392;&#3364;&#3391; &#3337;&#3370;&#3375;&#3399;&#3351;&#3391;&#3349;&#3405;&#3349;&#3393;&#3349;.&#3370;&#3391;&#3368;&#3405;&#3368;&#3398; &#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3377;&#3391;&#3368;&#3405;&#3377;&#3398; &#3370;&#3399;&#3376;&#3405;,&#3381;&#3391;&#3381;&#3376;&#3363;&#3330;,&#3364;&#3376;&#3330; &#3342;&#3368;&#3405;&#3368;&#3391;&#3381; &#3368;&#3378;&#3405;&#8205;&#3349;&#3393;&#3349;.&#3370;&#3391;&#3368;&#3405;&#3368;&#3398; &#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3378;&#3391;&#3384;&#3405;&#3377;&#3405;&#3377;&#3405;  &#3368;&#3403;&#3349;&#3405;&#3349;&#3391; &#3333;&#3330;&#3382;&#3330;.&#3355;&#3399;&#3366;&#3330; &#3342;&#3368;&#3405;&#3368;&#3391;&#3381; &#3349;&#3405;&#3379;&#3391;&#3349;&#3405;&#3349;&#3405; &#3354;&#3398;&#3375;&#3405;&#3364;&#3405; &#3371;&#3403;&#3376;&#3405;&#8205;&#3374;&#3393;&#3378; &#3368;&#3391;&#3376;&#3405;&#8205;&#3381;&#3354;&#3391;&#3349;&#3405;&#3349;&#3393;&#3349;.</p>

<h4>&#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3371;&#3403;&#3376;&#3405;&#8205;&#3374;&#3393;&#3378; &#3368;&#3391;&#3376;&#3405;&#8205;&#3381;&#3354;&#3391;&#3349;&#3405;&#3349;&#3393;&#3349;</h4>
<p>&#3333;&#3330;&#3382;&#3364;&#3405;&#3364;&#3391;&#3368;&#3393;&#3330; &#3354;&#3399;&#3366;&#3364;&#3405;&#3364;&#3391;&#3368;&#3393;&#3374;&#3393;&#3379;&#3405;&#3379; &#3371;&#3403;&#3376;&#3405;&#8205;&#3374;&#3393;&#3378; &#3370;&#3405;&#3376;&#3364;&#3399;&#3349;&#3330; &#3368;&#3391;&#3376;&#3405;&#8205;&#3381;&#3354;&#3391;&#3349;&#3405;&#3349;&#3363;&#3330;. &#3335;&#3368;&#3405;&#8205;&#3361;&#3391;&#3349;&#3405;&#3349;&#3399;&#3377;&#3405;&#3377;&#3376;&#3405;&#8205; &#3371;&#3403;&#3376;&#3405;&#8205;&#3374;&#3393;&#3378;&#3349;&#3405;&#3349;&#3405; &#3381;&#3399;&#3363;&#3405;&#3359;&#3391;&#3375;&#3393;&#3379;&#3405;&#3379; &#3370;&#3403;&#3370;&#3405;&#3370;&#3405; &#3334;&#3370;&#3405; &#3356;&#3390;&#3378;&#3349;&#3364;&#3405;&#3364;&#3391;&#3378;&#3405;&#8205; &#3371;&#3403;&#3376;&#3405;&#8205;&#3374;&#3393;&#3378;&#3375;&#3393;&#3330; &#3378;&#3373;&#3405;&#3375;&#3374;&#3390;&#3375; &#3347;&#3370;&#3377;&#3398;&#3377;&#3405;&#3377;&#3377;&#3393;&#3349;&#3379;&#3393;&#3330; &#3335;&#3359;&#3364;&#3405;&#3364;&#3405; &#3374;&#3393;&#3378;&#3375;&#3391;&#3378;&#3405;&#8205; &#3349;&#3391;&#3359;&#3405;&#3359;&#3393;&#3330;. &#3378;&#3373;&#3405;&#3375;&#3374;&#3390;&#3375; &#3342;&#3378;&#3405;&#3378;&#3390; &#3361;&#3390;&#3377;&#3405;&#3377;&#3390; &#3342;&#3378;&#3391;&#3374;&#3398;&#3368;&#3405;&#3377;&#3405;&#3384;&#3393;&#3330; &#3333;&#3359;&#3393;&#3349;&#3405;&#3349;&#3391; &#3349;&#3393;&#3359;&#3405;&#3359;&#3374;&#3390;&#3375;&#3391;&#3359;&#3405;&#3359;&#3393;&#3379;&#3405;&#3379; &#3347;&#3370;&#3377;&#3398;&#3377;&#3405;&#3377;&#3377;&#3393;&#3349;&#3379;&#3393;&#3330; &#3335;&#3359;&#3364;&#3405;&#3364;&#3405; &#3381;&#3382;&#3364;&#3405;&#3364;&#3405; &#3368;&#3391;&#3376;&#3364;&#3405;&#3364;&#3391;&#3359;&#3405;&#3359;&#3393;&#3363;&#3405;&#3359;&#3405; . &#3361;&#3390;&#3377;&#3405;&#3377;&#3390; &#3342;&#3378;&#3391;&#3374;&#3398;&#3368;&#3405;&#3377;&#3405;&#3384;&#3393;&#3330; &#3349;&#3393;&#3359;&#3405;&#3359;&#3391;&#3354;&#3399;&#3376;&#3405;&#8205;&#3349;&#3405;&#3349;&#3363;&#3374;&#3398;&#3353;&#3405;&#3349;&#3391;&#3378;&#3405;&#8205; &#3371;&#3403;&#3376;&#3405;&#8205;&#3374;&#3393;&#3378;&#3375;&#3391;&#3378;&#3405;&#8205; &#3376;&#3363;&#3405;&#3359;&#3405; &#3370;&#3405;&#3376;&#3390;&#3381;&#3382;&#3405;&#3375;&#3330; &#3370;&#3405;&#3376;&#3384;&#3405;&#3384;&#3405; &#3354;&#3398;&#3375;&#3405;&#3375;&#3363;&#3330;, &#3361;&#3390;&#3377;&#3405;&#3377;&#3390; &#3342;&#3378;&#3391;&#3374;&#3398;&#3368;&#3405;&#3377;&#3405;&#3384;&#3391;&#3368;&#3405;&#3377;&#3398; &#3335;&#3359;&#3375;&#3405;&#3349;&#3405;&#3349;&#3405; &#3354;&#3399;&#3376;&#3405;&#8205;&#3349;&#3405;&#3349;&#3363;&#3374;&#3398;&#3353;&#3405;&#3349;&#3391;&#3378;&#3405;&#8205; &#3347;&#3370;&#3377;&#3398;&#3377;&#3405;&#3377;&#3377;&#3391;&#3378;&#3405;&#8205; &#3349;&#3405;&#3379;&#3391;&#3349;&#3405;&#3349;&#3405; &#3354;&#3398;&#3375;&#3405;&#3375;&#3393;&#3349;. &#3356;&#3390;&#3378;&#3349;&#3364;&#3405;&#3364;&#3391;&#3378;&#3405;&#8205; &#3349;&#3405;&#3379;&#3391;&#3349;&#3405;&#3349;&#3405; &#3354;&#3398;&#3375;&#3405;&#3375;&#3393;&#3364;&#3393; &#3349;&#3402;&#3363;&#3405;&#3359;&#3405; &#3371;&#3403;&#3376;&#3405;&#8205;&#3374;&#3393;&#3378; &#3356;&#3390;&#3378;&#3349;&#3364;&#3405;&#3364;&#3391;&#3378;&#3398;&#3349;&#3405;&#3349;&#3405; &#3349;&#3376;&#3405;&#8205;&#3384;&#3405;&#3384;&#3376;&#3405;&#8205; &#3368;&#3391;&#3349;&#3405;&#3349;&#3393;&#3349;&#3375;&#3393;&#3330; &#3334;&#3349;&#3377;&#3405;&#3377;&#3391;&#3381;&#3398;&#3377;&#3405;&#3377;&#3405; &#3354;&#3398;&#3375;&#3405;&#3375;&#3393;&#3349;&#3375;&#3393;&#3330;, &#3336; &#3349;&#3376;&#3405;&#8205;&#3384;&#3405;&#3384;&#3376;&#3405;&#8205; &#3342;&#3381;&#3391;&#3359;&#3398;&#3375;&#3390;&#3363;&#3402; &#3333;&#3359;&#3393;&#3364;&#3405;&#3364; &#3361;&#3390;&#3377;&#3405;&#3377;&#3390; &#3342;&#3378;&#3391;&#3374;&#3398;&#3368;&#3405;&#3377;&#3405;&#3384;&#3405; &#3381;&#3398;&#3375;&#3405;&#3375;&#3349;&#3398;&#3363;&#3405;&#3359;&#3364;&#3405;&#3364;&#3405; &#3342;&#3368;&#3405;&#3368;&#3393; &#3368;&#3391;&#3375;&#3368;&#3405;&#3364;&#3405;&#3376;&#3391;&#3349;&#3405;&#3349;&#3393;&#3330;. &#3333;&#3330;&#3382;&#3364;&#3405;&#3364;&#3391;&#3368;&#3393;&#3330; &#3354;&#3399;&#3366;&#3364;&#3405;&#3364;&#3391;&#3368;&#3393;&#3374;&#3393;&#3379;&#3405;&#3379; &#3346;&#3376;&#3393; &#3381;&#3391;&#3381;&#3376;&#3363;&#3330; &#3359;&#3398;&#3384;&#3405;&#3377;&#3405;&#3377;&#3375;&#3391; &#3342;&#3380;&#3393;&#3364;&#3405;&#3364;&#3363;&#3330;.  &#3334;&#3370;&#3405;&#3361;&#3399;&#3377;&#3405;&#3377;&#3405; &#3372;&#3359;&#3405;&#3359;&#3368;&#3391;&#3378;&#3405;&#8205; &#3371;&#3403;&#3376;&#3405;&#8205;&#3374;&#3393;&#3378; &#3384;&#3393;&#3349;&#3405;&#3383;&#3391;&#3349;&#3405;&#3349;&#3393;&#3368;&#3405;&#3368;&#3364;&#3391;&#3368;&#3405; &#3381;&#3399;&#3363;&#3405;&#3359;&#3391; &#3349;&#3405;&#3379;&#3391;&#3349;&#3405;&#3349;&#3405; &#3354;&#3398;&#3375;&#3405;&#3375;&#3393;&#3349; &#3333;&#3364;&#3391;&#3368;&#3405; &#3382;&#3399;&#3383;&#3330; &#3349;&#3405;&#3379;&#3403;&#3384;&#3405;&#3384;&#3405; &#3372;&#3359;&#3405;&#3359;&#3368;&#3391;&#3378;&#3405;&#8205; &#3349;&#3405;&#3379;&#3391;&#3349;&#3405;&#3349;&#3405; &#3354;&#3398;&#3375;&#3405;&#3364;&#3393; &#3349;&#3405;&#3379;&#3403;&#3384;&#3405;&#3384;&#3405;   &#3354;&#3398;&#3375;&#3405;&#3375;&#3393;&#3349;.</p>

